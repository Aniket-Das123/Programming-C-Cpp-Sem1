/*Program to convert string from uppor to lowercase*/
#include<stdio.h>
#include<string.h>
int main()
{
	char str1[20];
	int i;
	printf("Enter the string: ");
	gets(str1);
	for(i=0;i<=strlen(str1);i++)
	{
		if(str1[i]>=65&&str1[i]<=90)
		str1[i]=str1[i]+32;
	}
	printf("\nLowercase string is : %s",str1);
	return 0;
}
